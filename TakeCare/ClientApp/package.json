{
  "name": "takecare-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prettier-verify": "prettier ./src --check --config ./.prettierrc.json",
    "debug": "vite --debug",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "html2pdf.js": "^0.10.2",
    "pinia": "^2.1.7",
    "sweetalert2": "^11.6.13",
    "vee-validate": "^4.12.7",
    "vue": "^3.4.26",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.3.2",
    "vuex": "^4.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.2.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "vite": "^5.2.11",
    "vite-plugin-mkcert": "^1.17.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
